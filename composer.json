{
  "name": "jules/blauwdruk",
  "type": "project",
  "description": "Laravel app blueprint",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://pakjes.vagebond.nl",
      "only": [
        "vagebond/*"
      ]
    }
  ],
  "require": {
    "php": "^8.4",
    "guzzlehttp/psr7": "^2.7.1",
    "inertiajs/inertia-laravel": "^2.0.4",
    "laravel/fortify": "^1.27",
    "laravel/framework": "^12.20.0",
    "laravel/prompts": "^0.3.6",
    "league/flysystem-aws-s3-v3": "^3.29",
    "league/flysystem-path-prefixing": "^3.28",
    "matomo/device-detector": "^6.4.6",
    "mvanduijker/laravel-mercure-broadcaster": "^3.7",
    "nesbot/carbon": "^3.10.1",
    "red-explosion/laravel-sqids": "^1.3.1",
    "spatie/laravel-backup": "^9.3.3",
    "spatie/laravel-multitenancy": "^4.0.5",
    "spatie/laravel-notification-log": "^1.3.2",
    "spatie/laravel-passkeys": "^1.0.11",
    "spatie/laravel-permission": "^6.20",
    "staudenmeir/laravel-cte": "^12.1",
    "symfony/console": "^7.3.1",
    "symfony/finder": "^7.3",
    "symfony/http-client": "^7.3.1",
    "symfony/http-foundation": "^7.3.1",
    "symfony/http-kernel": "^7.3.1",
    "symfony/postmark-mailer": "^7.3",
    "symfony/process": "^7.3",
    "symfony/yaml": "^7.3.1",
    "vagebond/aannemer": "^1.3",
    "vagebond/beeld": "^3.0",
    "vagebond/bijlagen": "^4.5",
    "vagebond/tus": "^2.4",
    "web-auth/webauthn-lib": "^5.2.2"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.15.4",
    "fakerphp/faker": "^1.24.1",
    "larastan/larastan": "^3.6",
    "laravel/pint": "^1.24",
    "laravel/telescope": "^5.10",
    "mockery/mockery": "^1.6.12",
    "nunomaduro/collision": "^8.8.2",
    "overtrue/phplint": "^9.6.2",
    "pestphp/pest": "^3.8.2",
    "pestphp/pest-plugin-arch": "^3.1.1",
    "pestphp/pest-plugin-laravel": "^3.2",
    "rector/rector": "^2.1.2",
    "shipmonk/composer-dependency-analyser": "^1.8.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan app:licenses:list"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
