{
  "name": "jules/blauwdruk",
  "type": "project",
  "description": "Laravel app blueprint",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://pakjes.vagebond.nl",
      "only": [
        "vagebond/*"
      ]
    }
  ],
  "require": {
    "php": "^8.3",
    "guzzlehttp/psr7": "^2.7",
    "inertiajs/inertia-laravel": "^1.3",
    "laravel/fortify": "^1.24",
    "laravel/framework": "^11.9",
    "laravel/prompts": "^0.1.25",
    "league/flysystem-aws-s3-v3": "^3.28",
    "league/flysystem-path-prefixing": "^3.28",
    "red-explosion/laravel-sqids": "^1.2",
    "spatie/laravel-backup": "^9.0",
    "spatie/laravel-multitenancy": "^4.0",
    "spatie/laravel-notification-log": "^1.3",
    "spatie/laravel-permission": "^6.9",
    "symfony/postmark-mailer": "^7.1",
    "symfony/yaml": "^7.1",
    "vagebond/aannemer": "^1.2",
    "vagebond/beeld": "^2.6",
    "vagebond/bijlagen": "^4.3",
    "vagebond/tus": "^2.3"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.14",
    "fakerphp/faker": "^1.23",
    "larastan/larastan": "^2.9",
    "laravel/pint": "^1.17",
    "laravel/telescope": "^5.2",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "overtrue/phplint": "^9.4",
    "phpunit/phpunit": "^11.0.1",
    "shipmonk/composer-dependency-analyser": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
